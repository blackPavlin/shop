{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc && vite build",
    "lint": "eslint lint",
    "lint:fix": "eslint lint --fix",
    "generate": "openapi --input ../api/openapi.v1.yaml --output ./src/api --client axios"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "element-plus": "^2.7.3",
    "pinia": "^3.0.2",
    "vue": "^3.4.27",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.2.0",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.5.3",
    "sass": "^1.89.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
